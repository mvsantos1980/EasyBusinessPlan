<p id="notice"><%= notice %></p>

<h1>Competitivenesses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @competitivenesses.each do |competitiveness| %>
      <tr>
        <td><%= link_to 'Show', competitiveness %></td>
        <td><%= link_to 'Edit', edit_competitiveness_path(competitiveness) %></td>
        <td><%= link_to 'Destroy', competitiveness, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Competitiveness', new_competitiveness_path %>
